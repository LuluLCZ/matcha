<script src="http://localhost:3306/semantic/dist/semantic.min.js"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.1.1/socket.io.js"></script>
<script src="http://localhost:3306/socket.io/socket.io.js"></script>

		<script>
			$("#menu-toggle").click(function(e) {
				e.preventDefault()
				$("#wrapper").toggleClass("toggled")
			})
			$(document).ready(function(){
				$('.right.menu.open').on("click",function(e){
					e.preventDefault()
					$('.ui.vertical.menu').toggle()
				})
			});
				$('.ui.dropdown')
				.dropdown({
				allowAdditions: true,
				apiSettings: {
					url: '//api.semantic-ui.com/tags/{query}'
				}
			})
		</script>
		<% if (!locals.log) { %>
			<script>
				var socket = "tchi"
			</script>
		<% } else  { %>
			<script>
				var socket = io.connect()
				socket.emit('connectionne', {login: '<%- me %>'})
				var parse = '<%- locals.login %>'
				socket.emit('parse', {
					login	: parse
				})
				socket.on('notif', function() {
					$('#testt').append("<div class='floating ui red label' style='top:10px;'>!</div>");
				})
				
			</script>
		<% } %>
</body>
